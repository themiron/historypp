PROJECTS = historypp.dll historypp_icons.dll
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

historypp.dll: historypp.dpr
  $(DCC)

historypp_icons.dll: historypp_icons.dpr
  $(DCC)


